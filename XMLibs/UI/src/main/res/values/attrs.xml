<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="staticAlpha" format="float" />
    <attr name="scrollingAlpha" format="float" />

    <declare-styleable name="StateView">
        <attr name="loadingView" format="reference" />
        <attr name="errorView" format="reference" />
        <attr name="emptyView" format="reference" />
        <attr name="noNetworkView" format="reference" />
    </declare-styleable>

    <declare-styleable name="pickerview">
        <attr name="wheelview_gravity">
            <enum name="center" value="17" />
            <enum name="left" value="3" />
            <enum name="right" value="5" />
        </attr>
        <attr name="wheelview_textSize" format="dimension" />
        <attr name="wheelview_textColorOut" format="color" />
        <attr name="wheelview_textColorCenter" format="color" />
        <attr name="wheelview_dividerColor" format="color" />
        <attr name="wheelview_lineSpacingMultiplier" format="float" />
    </declare-styleable>

    <declare-styleable name="FlowLayoutAttrs">
        <attr name="horizontalSpacing" />
        <attr name="verticalSpacing" />
        <attr name="itemSize" />
        <attr name="itemColor" />
        <attr name="backgroundResource" format="reference" />
        <attr name="textPaddingH" />
        <attr name="textPaddingV" />
        <attr name="singleLine" format="boolean" />
    </declare-styleable>

    <declare-styleable name="XmScrollBar">
        <attr name="xmdirection" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
        <attr name="staticAlpha" />
        <attr name="scrollingAlpha" />
    </declare-styleable>

    <declare-styleable name="XmScrollViewBar">
        <attr name="scroll_direction" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="MusicWaveView">
        <attr name="cellWidth" format="dimension" />
        <attr name="waveSpace" format="dimension" />
        <attr name="cellColor" format="color" />
        <attr name="waveDuration" format="float" />
        <attr name="rate" format="float" />
    </declare-styleable>

    <declare-styleable name="UnderLineTextView">
        <attr name="input_type" format="enum">
            <enum name="normal" value="0" />
            <enum name="passwd" value="1" />
        </attr>
        <attr name="length" format="integer" />
        <attr name="text_size_ui" format="dimension" />
        <attr name="text_color" format="color" />
        <attr name="line_width" format="dimension" />
        <attr name="line_color" format="color" />
        <attr name="lines_margin" format="dimension" />
        <attr name="line_text_margin" format="dimension" />
    </declare-styleable>

    <declare-styleable name="DiscreteScrollView">
        <attr name="dsv_orientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
        <attr name="drawing_order" format="boolean" />
    </declare-styleable>

    <attr name="borderRadius" format="dimension" />

    <attr name="type">
        <enum name="circle" value="0" />
        <enum name="round" value="1" />
    </attr>

    <declare-styleable name="RoundImageView">
        <attr name="borderRadius" />
        <attr name="type" />
    </declare-styleable>


    <declare-styleable name="LoadingIndicatorView">
        <attr name="minWidth" format="dimension" />
        <attr name="maxWidth" format="dimension" />
        <attr name="minHeight" format="dimension" />
        <attr name="maxHeight" format="dimension" />
        <attr name="indicatorName" format="string" />
        <attr name="indicatorColor" format="color" />
    </declare-styleable>

    <declare-styleable name="VerticalScrollBar">
        <attr name="staticAlpha" />
        <attr name="scrollingAlpha" />
    </declare-styleable>

    <declare-styleable name="ReflectionImageView">
        <attr name="reflectionHeight" format="dimension" />
        <attr name="reflectionPadding" format="dimension" />
    </declare-styleable>

    <declare-styleable name="WheelPickView">
        <attr name="divide_color" format="color" />
        <attr name="text_diff_size" format="boolean" />
        <attr name="selected_text_size" format="dimension" />
        <attr name="unselected_text_size" format="dimension" />
    </declare-styleable>
</resources>