<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">


    <ImageView
        android:id="@+id/location_image_bg"
        android:layout_width="@dimen/width_friend_bg"
        android:layout_height="@dimen/width_friend_bg"
        android:background="@drawable/bg_location_share_friend" />

    <ImageView
        android:id="@+id/iv_user_head"
        android:layout_width="@dimen/width_friend_head"
        android:layout_height="@dimen/width_friend_head"
        android:layout_gravity="center" />

</FrameLayout>