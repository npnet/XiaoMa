<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xiaoma.songname">

    <application
        android:name=".SongName"
        android:allowBackup="true"
        android:icon="@drawable/icon_default_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/XiaoMaTheme">

        <meta-data
            android:name="CarType"
            android:value="XMLX" />

        <meta-data
            android:name="CarChannelId"
            android:value="${CAR_CHANNEL_ID}" />

        <meta-data
            android:name="XmGitRev"
            android:value="${XM_GIT_REV}"/>

        <activity android:name=".ui.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ui.activity.SortActivity" />

    </application>

</manifest>