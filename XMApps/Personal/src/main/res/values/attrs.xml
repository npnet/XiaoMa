<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Signin">
        <attr name="sign_in_bg_color" format="color" />
        <attr name="sign_in_pb_color" format="color" />
        <attr name="sign_in_check_color" format="color" />
        <attr name="sign_in_text_color" format="color" />
        <attr name="sign_in_text_siz" format="dimension" />
    </declare-styleable>

    <declare-styleable name="WheelView">
        <attr name="unit_text" format="string" />
    </declare-styleable>

    <declare-styleable name="StepView">
        <attr name="size" format="integer" />
    </declare-styleable>

    <declare-styleable name="XmProgressBar">
        <attr name="background_drawable" format="reference" />
        <attr name="max" format="integer" />
        <attr name="progress" format="integer" />
        <attr name="bar_color" format="color" />
        <attr name="bar_drawable" format="reference" />
        <attr name="bar_margin" format="dimension" />
        <attr name="background_color" format="color" />
    </declare-styleable>


    <!-- NestedScrollView bar -->
    <declare-styleable name="XMNestedScrollBar">
        <attr name="nested_scroll_bar_width" format="dimension" />
        <attr name="nested_scroll_bar_height" format="dimension" />
        <attr name="margin_top_bottom" format="dimension" />
    </declare-styleable>


    <declare-styleable name="SlideView">
        <attr name="SlideEnable" format="boolean" />
        <attr name="SlideLeft" format="boolean" />
        <attr name="SlideIntercept" format="boolean" />
        <attr name="SlideMargin" format="dimension" />
    </declare-styleable>
</resources>