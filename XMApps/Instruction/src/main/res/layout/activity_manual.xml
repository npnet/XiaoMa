<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:layout_marginLeft="@dimen/px_108">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/instruction"
        android:textSize="@dimen/px_37"
        android:layout_marginTop="@dimen/px_100"
        android:textColor="@color/white"
        android:textStyle="bold"/>

    <android.support.v7.widget.RecyclerView
        android:id="@+id/manual_catalog"
        android:layout_width="1592px"
        android:layout_height="537px"
        android:layout_marginTop="@dimen/px_170">
    </android.support.v7.widget.RecyclerView>

</FrameLayout>
