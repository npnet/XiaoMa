<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">


    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="6px" />
            <solid android:color="#3f434e" />
        </shape>

    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="6px" />
                <gradient
                    android:angle="270"
                    android:centerColor="#d2a652"
                    android:endColor="#a17c32"
                    android:startColor="#f2c266" />
                <stroke
                    android:width="1dp"
                    android:color="#3f434e" />
            </shape>
        </clip>
    </item>
</layer-list>
